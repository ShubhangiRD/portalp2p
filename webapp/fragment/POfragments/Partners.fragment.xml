<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table"
	xmlns:u="sap.ui.unified">
	<t:Table id="idPurchaseFunction" class="sapUiSmallMargin" rows="{ path: 'ParterDetails>/'}" visibleRowCount="5" selectionMode="MultiToggle">
		<t:extension>
			<OverflowToolbar>
				<Title text="Purchase Function "/>
				<ToolbarSpacer/>
				
			</OverflowToolbar>
		</t:extension>
		<t:columns>
			<t:Column width="9rem" hAlign="Center">
				<Label text="Furchase Function "/>
				<t:template>
					<Input id="idpurchasefun"  textFormatMode="KeyValue"  
					>
					</Input>
				</t:template>
			</t:Column>
			<t:Column hAlign="Center" width="9rem">
				<Label text="Name"/>
				<t:template>
					<Input  id="idnamepf"   value="{ParterDetails>Designation}" >
					</Input>
				</t:template>
			</t:Column>
			<t:Column width="11rem" hAlign="Center">
				<Label text="Number"/>
				<t:template>
					<Input id="idnumberpf" value="{ParterDetails>Vendor}" editable="{EditModel>/isEditable}"/>
				</t:template>
			</t:Column>
			<t:Column width="11rem" hAlign="Center">
				<Label text="Name"/>
				<t:template>
					<Input id="idnamepurfunc" value="{ParterDetails>Vendorname}" editable="{EditModel>/isEditable}"/>
				</t:template>
			</t:Column>
	
		</t:columns>
	</t:Table>
</core:FragmentDefinition>
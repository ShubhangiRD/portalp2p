<core:FragmentDefinition xmlns="sap.ui.table" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core">
	<TreeTable id="TreeTableBasic2" rows="{path:'oStockDataModel>/', parameters: {arrayNames:['MultipleIt','Plant']}}" enableSelectAll="false"
		ariaLabelledBy="title">
		<columns>
			<Column width="13rem">
				<m:Label text="Material"/>
				<template>
					<m:Text text="{oStockDataModel>Matnr}" wrapping="false"/>
				</template>
			</Column>
			<!--<Column width="9rem">-->
			<!--	<m:Label text="Plant"/>-->
			<!--	<template>-->
			<!--		<m:Text text="{oStockDataModel>Werks}"/>-->
			<!--	</template>-->
			<!--</Column>-->
			<Column width="9rem">
				<m:Label text="Company Code"/>
				<template>
					<m:Text text="{oStockDataModel>CompanyCode}"/>
				</template>
			</Column>
			<Column width="13rem">
				<m:Label text="Available Quantity"/>
				<template>
					<m:ObjectNumber number="{oStockDataModel>Labst}"
						state="{ parts: [ {path: 'oStockDataModel>Labst'}, {path: 'oStockDataModel>Crtlv'}, {path: 'oStockDataModel>Cytlv'}, {path: 'oStockDataModel>Cgtlv'}, {path: 'oStockDataModel>Cbtlv'}, {path: 'oStockDataModel>Matnr'} ], formatter: '.formatter.statusColor' }"/>
				</template>
			</Column>
			<Column width="13rem" sorted="true">
				<m:Label text="Critical Threshold"/>
				<template>
					<m:Text text="{oStockDataModel>Crtlv} "/>
				</template>
			</Column>
			<Column width="9rem">
				<m:Label text="Success"/>
				<template>
					<m:Text text="{oStockDataModel>Cgtlv}"/>
				</template>
			</Column>
			<Column width="9rem">
				<m:Label text="Warning"/>
				<template>
					<m:Text text="{oStockDataModel>Cytlv}"/>
					<m:ObjectNumber number="{ path : 'oStockDataModel>Red' }"
						state="{= ${oStockDataModel>CurrentThread} > ${oStockDataModel>Labst} ? 'Warning' : ${oStockDataModel>Labst} > ${oStockDataModel>CurrentThread} ? 'Error' : 'Success'}"/>
				</template>
			</Column>
			<Column width="13rem" sorted="true">
				<m:Label text="Warrning Threshold"/>
				<template>
					<m:Text text="{oStockDataModel>Cbtlv}"/>
					<m:ObjectNumber number="{ path : 'oStockDataModel>Yellow' }"
						state="{= ${oStockDataModel>CurrentThread} > ${oStockDataModel>Labst} ? 'Warning' : ${oStockDataModel>Labst} > ${oStockDataModel>CurrentThread} ? 'Error' : 'Success'}"/>
				</template>
			</Column>
			<!--<Column width="13rem" sorted="true">-->
			<!--	<m:Label text="Current Threshold"/>-->
			<!--	<template>-->
			<!--		<m:ObjectNumber number="{ path : 'oStockDataModel>CurrentThread' }"-->
			<!--			state="{= ${oStockDataModel>CurrentThread} > ${oStockDataModel>Labst} ? 'Warning' : ${oStockDataModel>Labst} > ${oStockDataModel>CurrentThread} ? 'Error' : 'Success'}"/>-->
			<!--	</template>-->
			<!--</Column>-->
			<!--<Column width="13rem">-->
			<!--	<m:Label text="Previous Threshold"/>-->
			<!--	<template>-->
			<!--		<m:Text text="{oStockDataModel>PreviousThread}" wrapping="false"/>-->
			<!--	</template>-->
			<!--</Column>-->
			<Column width="13rem" sorted="true">
				<m:Label text="Green Threshold"/>
				<template>
					<m:ObjectNumber number="{ path : 'oStockDataModel>Green' }"
						state="{= ${oStockDataModel>CurrentThread} > ${oStockDataModel>Labst} ? 'Warning' : ${oStockDataModel>Labst} > ${oStockDataModel>CurrentThread} ? 'Error' : 'Success'}"/>
				</template>
			</Column>
			<Column width="13rem" sorted="true">
				<m:Label text="Excess Threshold"/>
				<template>
					<m:ObjectNumber number="{ path : 'oStockDataModel>Blue' }"
						state="{= ${oStockDataModel>CurrentThread} > ${oStockDataModel>Labst} ? 'Warning' : ${oStockDataModel>Labst} > ${oStockDataModel>CurrentThread} ? 'Error' : 'Success'}"/>
				</template>
			</Column>
			<Column width="13rem" sorted="true" visible="false">
				<m:Label text="Color"/>
				<template>
					<m:ObjectIdentifier title="{path: 'oStockDataModel>Color'}"/>
				</template>
			</Column>
			<Column width="13rem">
				<m:Label text="Date"/>
				<template>
					<m:Text text="{ path: 'oStockDataModel>Changedon', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'yyyy-MM-dd' } }"
						wrapping="false"/>
				</template>
			</Column>
		</columns>
	</TreeTable>
</core:FragmentDefinition>
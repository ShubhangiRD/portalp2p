<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Product Hierarchy"
		class="sapUiPopupWithPadding"
		items="{Lookup>/ProductHierarchy}"
		search="_handleProductHierarchySearch"
		confirm="_handleProductHierarchyClose"
		cancel="_handleProductHierarchyClose">
		<StandardListItem
			title="{Lookup>Prodh}"
			description="{Lookup>Vtext}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
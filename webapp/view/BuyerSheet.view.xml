	<layout:Grid class="sapUiSmallMarginTop" containerQuery="true" vSpacing="0" defaultSpan="XL12 L12 M12 S12">
		<layout:Grid containerQuery="true" vSpacing="1rem" hSpacing="0" defaultSpan="XL6 L6 M6 S6">
			<layout:FixFlex id='chartFixFlex2' minFlexSize="500">
				<!--Below is the setting panel -->
				<layout:fixContent></layout:fixContent>
				<layout:flexContent>
					<viz:Popover id="idPopOver1"></viz:Popover>
					<viz:VizFrame id="idVizFrame1" uiConfig="{applicationSet:'fiori'}" vizProperties="{ title: {text : 'Stock vs Sales', visible : true}}"
						height='100%' width="100%" vizType='combination'>
						<viz:dataset>
							<viz.data:FlattenedDataset data="{oStockDataModel>/}">
								<viz.data:dimensions>
									<viz.data:DimensionDefinition name="Material" value="{Matnr}"/>
								</viz.data:dimensions>
								<viz.data:measures>
									<viz.data:MeasureDefinition name="Revenue" value="{OpenSalesOrder}"/>
									<viz.data:MeasureDefinition name="Stock in Hand" value="{Labst}"/>
									<viz.data:MeasureDefinition name="Cost2" value="{OpenSalesOrder}"/>
									<viz.data:MeasureDefinition name="Cost1" value="{OpenSalesOrder}"/>
								</viz.data:measures>
							</viz.data:FlattenedDataset>
						</viz:dataset>
						<viz:feeds>
							<viz.feeds:FeedItem uid="valueAxis" type="Measure" values="Revenue"/>
							<viz.feeds:FeedItem uid="valueAxis" type="Measure" values="Stock in Hand"/>
							<viz.feeds:FeedItem id="feedCategoryAxis1" uid="categoryAxis" type="Dimension" values="Material"/>
						</viz:feeds>
					</viz:VizFrame>
				</layout:flexContent>
			</layout:FixFlex>
			<layout:FixFlex id='chartFixFlex' minFlexSize="500">
				<!--Below is the setting panel -->
				<layout:flexContent>
					<viz:Popover id="idPopOver"></viz:Popover>
					<viz:VizFrame id="idVizFrame" uiConfig="{applicationSet:'fiori'}"
						vizProperties="{ title: {text : 'ForeCast Sales for Demand', visible : true}}" height='100%' width="100%" vizType='line'>
						<viz:dataset>
							<viz.data:FlattenedDataset data="{oSaleModel>/}">
								<viz.data:dimensions>
									<viz.data:DimensionDefinition name="Material" value="{oSaleModel>Matnr}"/>
								</viz.data:dimensions>
								<viz.data:measures>
									<viz.data:MeasureDefinition name="Demand" value="{oSaleModel>Kwmeng}"/>
									<viz.data:MeasureDefinition name="Cost" value="{oSaleModel>Matnr}"/>
								</viz.data:measures>
							</viz.data:FlattenedDataset>
						</viz:dataset>
						<viz:feeds>
							<viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure" values="Demand"/>
							<viz.feeds:FeedItem id='categoryAxisFeed' uid="categoryAxis" type="Dimension" values="Material"/>
						</viz:feeds>
					</viz:VizFrame>
				</layout:flexContent>
			</layout:FixFlex>
